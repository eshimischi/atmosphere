{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "brick/money": "^0.6.0",
        "doctrine/dbal": "^3.3",
        "ernysans/laraworld": "^1.0",
        "freesgen/atmosphere": "dev-main",
        "google/apiclient": "^2.12",
        "guzzlehttp/guzzle": "^7.0.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "insane/journal": "dev-master",
        "kreait/laravel-firebase": "^5.2",
        "laravel/cashier-paddle": "^1.8",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^9.0",
        "php-mime-mail-parser/php-mime-mail-parser": "^7.1",
        "phpoffice/phpspreadsheet": "^1.27",
        "simshaun/recurr": "^5.0",
        "spatie/laravel-onboard": "^2.4",
        "staudenmeir/belongs-to-through": "^2.5",
        "symfony/dom-crawler": "^6.0",
        "tightenco/ziggy": "^1.5.0"
    },
    "repositories": [
        {
          "url": "../packages/journal",
          "type": "path",
          "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.9.1",
        "insane/remotr": "dev-main",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Google\\Task\\Composer::cleanup",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "composer install --prefer-dist --no-interaction --no-scripts --no-progress --no-ansi --ignore-platform-reqs",
            "composer dump-autoload",
            "composer install --prefer-dist --no-interaction --no-scripts --no-suggest --no-progress --no-ansi --ignore-platform-reqs"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Gmail",
            "Calendar"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
